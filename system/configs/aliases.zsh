# Cube development aliases
alias canary='open -a "Google Chrome Canary" --args --disable-web-security --user-data-dir=$HOME/brandon.porter --allow-file-access-from-files --disable-extensions-http-throttling --allow-file-access --enable-file-cookies'

alias chrome='open -a "Google Chrome" --args --disable-web-security --user-data-dir=$HOME/brandon.porter --allow-file-access-from-files --disable-extensions-http-throttling --allow-file-access --enable-file-cookies'

